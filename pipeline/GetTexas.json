					"source": {
						"type": "ExcelSource",
						"additionalColumns": [
							{
								"name": "STATE",
								"value": "TEXAS"
							}
						],
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
